{
    "name": "torreswil/carbon-colombia",
    "type": "library",
    "description": "Extension de nesbot/carbon, para trabajar con festivos de Colombia",
    "keywords": [
        "torreswil",
        "carbon-colombia"
    ],
    "homepage": "https://github.com/torreswil/carbon-colombia",
    "license": "MIT",
    "authors": [
        {
            "name": "Wilfredo Torres",
            "email": "wtorresariza@gmail.com",
            "homepage": "http://wilto.co",
            "role": "Developer"
        }
    ],
    "require": {
        "nesbot/carbon": "^2"
    },
    "autoload": {
        "psr-4": {
            "torreswil\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "torreswil\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
